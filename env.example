# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# API Configuration
API_ENDPOINT=http://localhost:8787

# HMAC Secret (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
HMAC_SECRET=your-256-bit-hmac-secret-here

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Optional: Rate limiting and payload size
RATE_LIMIT_PER_MINUTE=60
MAX_PAYLOAD_SIZE=50000

# Supabase PostGres
DATABASE_URL=stuff